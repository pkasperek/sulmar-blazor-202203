@page "/products"
@inject IProductRepository productRepository

<h3>Products</h3>

<ul>
    @foreach(var product in products)
    {
        <li>@product.Name</li>
    }
</ul>

@code {
    private IEnumerable<Product> products;


    protected override async Task OnInitializedAsync()
    {
        products = await productRepository.GetAsync();
    }
}





