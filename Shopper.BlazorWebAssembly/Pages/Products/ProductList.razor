@page "/products"

@inject IProductService productService

<h3>Product List</h3>

@if (products != null)
{
    <ul>
        @foreach (var product in products)
        {
            <li>@product.Name</li>
        }
    </ul>
}

@code {

    private IEnumerable<Product> products;

    protected override async Task OnInitializedAsync()
    {
        products = await productService.GetAsync();
    }
}
